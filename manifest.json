{
  "manifest_version": 3,
  "name": "PhishTension",
  "version": "1.0",
  "description": "Senior Project for BS CpE. This extension is using manifest_version 3",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "PhishTension",
    "default_popup": "popup.html"
  }
}